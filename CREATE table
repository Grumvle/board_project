create table BOARD(
    board_idx int primary key auto_increment,
    board_writer varchar(30),
    board_title varchar(50),
    board_content varchar(2400),
    board_viewcnt int,
    board_likecnt int,
    board_pwd int,
    board_date date,
    board_newdate date
);

Create table member(
	name varchar(100),
	job varchar(100),
	id varchar(200) primary key,
	pwd varchar(200),
	phone varchar(200),
	addr varchar(500)
);
